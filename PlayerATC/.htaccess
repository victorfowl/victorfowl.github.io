<IfModule mod_mime.c="">
	AddType jugador/wasm .wasm
	AddType jugador/octet-stream .data
	AddType jugador/javascript .js
	AddType jugador/octet-stream .unityweb
	AddType jugador/octet-stream .mem
</IfModule>

<IfModule mod_deflate.c="">
	AddOutputFilterByType DEFLATE jugador/wasm
</IfModule>

<FilesMatch "\.(data|wasm|js|mem|unityweb)$">
	Header set Cache-Control "max-age=31536000, public"
</FilesMatch>